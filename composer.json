{
	"name": "denkmal/denkmal.org",
	"require": {
		"cargomedia/cm": "~1.3.7"
	},
	"minimum-stability": "alpha",
	"repositories": [
		{
			"packagist": false
		},
		{
			"type": "composer",
			"url": "http://satis.cargomedia.ch/dist"
		}
	],
	"autoload": {
		"psr-0": {
			"Denkmal_": "library/Denkmal/library/",
			"Admin_": "library/Admin/library/"
		}
	},
	"extra": {
		"cm-modules": {
			"Denkmal": {
				"path": "library/Denkmal/"
			},
			"Admin": {
				"path": "library/Admin/"
			}
		}
	}
}
