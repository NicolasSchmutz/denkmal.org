{
  "name": "denkmal/denkmal.org",
  "config": {
    "bin-dir": "bin/"
  },
  "require": {
    "cargomedia/cm": "~1.82.0",
    "ricardoper/twitteroauth": "~1.1.1",
    "nojimage/twitter-text-php": "~1.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.1.3",
    "tomaszdurka/mocka": "~0.7.0",
    "fzaninotto/faker": "~1.4.0"
  },
  "minimum-stability": "alpha",
  "repositories": [
    {
      "type": "composer",
      "url": "http://satis.cargomedia.ch/dist"
    }
  ],
  "autoload": {
    "psr-0": {
      "Denkmal_": "library/Denkmal/library/",
      "Admin_": "library/Admin/library/"
    }
  },
  "extra": {
    "cm-modules": {
      "Denkmal": {
        "path": "library/Denkmal/"
      },
      "Admin": {
        "path": "library/Admin/"
      }
    }
  }
}
